{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Jwt": {
    "Audience": "my-dotnet-logger-api",
    "Issuer": "me",
    "SecretKey": "my-dotnet-logger-api-secret-key",
    "Alg": "HS256",
    "ValidForInMinitues": 30,
    "RefreshTokenValidForInDays": 30,
    "LockUserJwtInMinitues": 60,
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidateIssuerSigningKey": true,
    "RequireExpirationTime": true,
    "ValidateLifetime": true,
    "AuthenticationScheme": "Bearer",
    "TokenType": "JWT"
  }
}